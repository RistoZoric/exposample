{
    "name": "privy",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "tsc",
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch --mix-config=webpack.mix.js",
        "watch-v2": "mix watch --mix-config=webpack.v2.mix.js",
        "watch-poll": "mix watch",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "prod-v2": "npm run production-v2",
        "production": "mix --mix-config=webpack.mix.js --production",
        "production-v2": "mix --mix-config=webpack.v2.mix.js --production"
    },
    "dependencies": {
        "@chakra-ui/icons": "^1.0.17",
        "@chakra-ui/react": "^1.8.1",
        "@chakra-ui/theme-tools": "^1.2.3",
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.1.5",
        "@inertiajs/inertia": "^0.9.2",
        "@inertiajs/inertia-react": "^0.6.2",
        "@inertiajs/progress": "^0.2.7",
        "@reduxjs/toolkit": "^1.6.2",
        "@stripe/react-stripe-js": "^1.6.0",
        "@stripe/stripe-js": "^1.21.1",
        "axios": "^0.21.4",
        "chart.js": "^3.6.0",
        "framer-motion": "^3.10.6",
        "jquery": "^3.6.0",
        "lang.js": "^1.1.14",
        "lodash": "^4.17.21",
        "mathjs": "^9.5.2",
        "react": "^17.0.1",
        "react-chartjs-2": "^3.3.0",
        "react-cookie-consent": "^6.4.1",
        "react-corner-ribbon": "^0.0.5",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "react-html-parser": "^2.0.2",
        "react-icons": "^4.3.1",
        "react-player": "^2.9.0",
        "react-redux": "^7.2.6",
        "react-responsive-masonry": "^2.1.2",
        "react-scroll": "^1.8.2",
        "react-slick": "^0.29.0",
        "react-stars": "^2.2.5",
        "slick-carousel": "^1.8.1",
        "sweetalert": "^2.1.2",
        "vanilla-lazyload": "^17.5.0"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.16.7",
        "autoprefixer": "^10.4.2",
        "eslint": "^7.20.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "laravel-mix": "^6.0.41",
        "postcss": "^8.4.6",
        "tailwindcss": "^3.0.18"
    },
    "eslintConfig": {
        "extends": [
            "airbnb"
        ],
        "rules": {
            "react/react-in-jsx-scope": "off",
            "react/jsx-filename-extension": "off",
            "react/prop-types": "off",
            "react/jsx-props-no-spreading": "off",
            "no-undef": "off",
            "no-unused-vars": "off",
            "global-require": "off",
            "no-param-reassign": "off",
            "jsx-a11y/anchor-is-valid": "off",
            "no-console": "off",
            "max-len": "off",
            "import/no-extraneous-dependencies": "off",
            "import/no-unresolved": "off",
            "react/no-children-prop": "off",
            "no-plusplus": "off",
            "import/extensions": "off",
            "react/jsx-no-bind": "off",
            "camelcase": "off",
            "no-cap": "off"
        }
    }
}
